{
  "name": "ggshield",
  "displayName": "ggshield",
  "description": "GitGuardian's ggshield integration",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "ggshield", 
      "properties": {
        "ggshield.ggshieldPath": {
          "type": "string",
          "default": "/usr/local/bin/ggshield",
          "markdownDescription": "Path to ggshield executable (use `$ where ggshield` to find out)"
        },
        "ggshield.apiKey": {
          "type": "string",
          "default": null,
          "description": "GitGuardian Internal Monitoring API key"
        },
        "ggshield.apiUrl": {
          "type": "string",
          "default": "https://api.gitguardian.com/",
          "description": "GitGuardian Internal Monitoring URL (default: https://api.gitguardian.com/)"
        }
      }
    },
    "commands": [
      {
        "command": "ggshield.quota",
        "title": "ggshield: Show quota"
      },
       {
        "command": "ggshield.ignore",
        "title": "ggshield: Ignore last found incidents"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {}
}
